<div class="background dispindexb">
<% if flash[:success] %>
  <div class="success flash-bar col flex-container site-wrapper--padding">
    <p class="flash-type col col--full col--center"><%= flash[:success] %></p>
  </div>
<% end %>

<% @user.dispensers.each do |disp| %>


<h1><%= link_to(disp.name, dispenser_plants_path(disp)) %></h1>


<% disp.plants.each do |plant| %>
<ol>
  <li><%= plant.name %>, <%= plant.location %></li>
</ol>
<% end %>
<% end %>
</div>
